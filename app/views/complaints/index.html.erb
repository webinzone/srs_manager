<div class="main-panel">
          <div class="content-wrapper">
            <div class="page-header">
              <h3 class="page-title"><b>Complaints</b></h3>
             
            </div>
            <div class="row">
              <div class="col-12 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                   <%= form_with(model: @complaint, remote: true , class: "forms-sample") do |f| %>
                      <div class="form-group">
                        <label for="exampleInputDate">Date</label>
                        <input type="text" class="form-control" id="exampleInputDate" placeholder="Date" />
                      </div>
                      <div class="form-group">
                        <label for="exampleInputComplaints">Complaints</label>
                        <input type="text" class="form-control" id="exampleInputComplaints" placeholder="Complaints" />
                      </div>
                      <div class="form-group">
                        <label for="exampleInputNature">Nature of the Compaints</label>
                        <input type="text" class="form-control" id="exampleInputNature" placeholder="Nature of the Compaints" />
                      </div>
                       <div class="form-group">
                        <label for="exampleInputNature">Action Taken</label>
                        <input type="text" class="form-control" id="exampleInputNature" placeholder="Action Taken" />
                      </div>
                       <div class="form-group">
                        <label for="exampleInputNature">Methd of communication of the outcome of the complaints</label>
                        <input type="text" class="form-control" id="exampleInputNature" placeholder="Methd of communication" />
                      </div>
                      <button type="submit" class="btn btn-primary mr-2"> Submit </button>
                      <button class="btn btn-light">Cancel</button>
                    <% end %>        
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
