<%= form_with(model: @file_report, multipart: true, class: "forms-sample") do |f| %>
  <div class="form-group">
    <label for="exampleInputuser">User Name</label>
   <%= f.text_field :user_name, autofocus: true,  :class => "form-control", placeholder:"User Name" %>
  </div>
  <div class="form-group">
    <label for="exampleInputfname">Tittle</label>
    <%= f.text_field :tittle, autofocus: true,  :class => "form-control", placeholder:"Tittle" %>
  </div>
  <div class="form-group">
    <label for="exampleInputdesc">Description</label><br>
    <%#= text_area_tag(:desc, :text, size: '90x10', autofocus: true) %>
    <%= f.text_field :desc, autofocus: true,  :class => "form-control", placeholder:"Description" %>
  </div>
  <div class="form-group">
    <label>Choose File</label>
    <%#= f.file_field :file_name,  :class => "form-control" %>
    <%= file_field_tag :file_name , id:"file_name",:class => "form-control" %>
   
  </div>
  <button type="submit" class="btn btn-primary mr-2"> Submit </button>
  <button class="btn btn-light">Cancel</button
<% end %>  